{
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "website",
  "devCommand": "cd website && python3 -m http.server 3000",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/docs",
      "destination": "/docs/index.html"
    },
    {
      "source": "/docs/:path*",
      "destination": "/docs/:path*.html"
    }
  ]
}
