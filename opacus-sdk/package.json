{
  "name": "@brienteth/opacus-sdk",
  "version": "1.0.0",
  "description": "Opacus - Multi-Chain Decentralized Agent Communication Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.esm.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "prepublish": "npm run build"
  },
  "keywords": [
    "opacus",
    "blockchain",
    "0g-chain",
    "multi-chain",
    "agent-communication",
    "decentralized",
    "web3",
    "dac"
  ],
  "author": "Opacus Team",
  "license": "MIT",
  "dependencies": {
    "@noble/ed25519": "^2.0.0",
    "@noble/curves": "^1.4.0",
    "@noble/hashes": "^1.4.0",
    "cbor-x": "^1.5.0",
    "ethers": "^6.11.0",
    "@0glabs/0g-ts-sdk": "^0.3.1",
    "@0glabs/0g-serving-broker": "^0.3.0",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Opacus-xyz/Opacus",
    "directory": "opacus-sdk"
  },
  "bugs": {
    "url": "https://github.com/Opacus-xyz/Opacus/issues"
  },
  "homepage": "https://newopacus.vercel.app"
}
